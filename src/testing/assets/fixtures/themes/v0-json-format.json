{
  "elements": {
    "message": {
      "foreground": "#ffffff",
      "modes": ["bold"]
    },
    "level": {
      "foreground": "#00ff00",
      "background": "#001100",
      "modes": ["italic", "underline"]
    },
    "level-inner": {
      "foreground": "#00ffff"
    },
    "time": {
      "foreground": "#aaaaaa",
      "modes": ["faint"]
    },
    "caller": {
      "foreground": "#cccccc"
    },
    "key": {
      "foreground": "#ffff00",
      "modes": ["bold"]
    },
    "value": {
      "foreground": "#00aaaa"
    },
    "boolean": {
      "foreground": "#00ff00",
      "modes": ["bold"]
    },
    "boolean-true": {
      "foreground": "#00ffff"
    },
    "boolean-false": {
      "foreground": "#ff0000"
    }
  },
  "levels": {
    "debug": {
      "level": {
        "foreground": "#ff00ff",
        "modes": ["faint"]
      }
    },
    "warning": {
      "level": {
        "foreground": "#ffaa00",
        "modes": ["bold"]
      },
      "time": {
        "foreground": "#ffaa00"
      }
    },
    "error": {
      "level": {
        "foreground": "#ff0000",
        "background": "#440000",
        "modes": ["reverse", "bold"]
      },
      "message": {
        "foreground": "#ff8888"
      }
    }
  },
  "indicators": {
    "sync": {
      "synced": {
        "text": " "
      },
      "failed": {
        "text": "!",
        "inner": {
          "style": {
            "foreground": "yellow",
            "modes": ["bold"]
          }
        }
      }
    }
  }
}
